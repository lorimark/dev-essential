<?xml version="1.0" encoding="UTF-8"?>
<messages xmlns:if="Wt.WTemplate.conditions">

  <message id="sysUser.detailForm">

    <div>User Detail Form</div>

    <div class="col-sm-6">  ${block:Wtx.detailForm.field keyField}      </div>
    <div class="col-sm-6">  ${block:Wtx.detailForm.field cfyField}      </div>
    <div class="col-sm-6">  ${block:Wtx.detailForm.field lastName}      </div>
    <div class="col-sm-6">  ${block:Wtx.detailForm.field firstName}     </div>
    <div class="col-sm-6">  ${block:Wtx.detailForm.field nickName}      </div>
    <div class="col-sm-6">  ${block:Wtx.detailForm.field email}         </div>

    <div class="col-sm-12"> ${block:Wtx.detailForm.field varField}      </div>

    <div class="col-sm-6"> ${block:Wtx.detailForm.field createdOn}     </div>
